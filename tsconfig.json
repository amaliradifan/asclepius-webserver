{
  "compilerOptions": {
    "target": "es6",
    "module": "commonjs",
    "outDir": "./dist",
    "rootDir": "./src",
    "strict": false,  // Coba matikan strict mode untuk melihat apakah itu membantu
    "esModuleInterop": true,
    "skipLibCheck": true  // Menonaktifkan pemeriksaan tipe untuk file definisi
  },
  "include": ["src/**/*"],
  "exclude": ["node_modules", "**/*.spec.ts"]
}
